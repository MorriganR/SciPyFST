digraph fst {
	rankdir=LR;
	node [shape=point]; start;
	node [shape=circle];
	"0" [label="0/-"];
	start -> "0" [label=start];
	node [shape=circle];
	"1" [label="1/0"];
	"2" [label="2/0"];
	"3" [label="3/1"];
	node [style=filled, fillcolor=hotpink];
	"0" -> "1" [label=0];
	"0" -> "2" [label=1];
	"2" -> "1" [label=0];
	"2" -> "2" [label=1];
	"3" -> "1" [label=0];
	"3" -> "2" [label=1];
	"1" -> "1" [label=0];
	"1" -> "3" [label=1];
}
